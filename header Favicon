 <?php
  $favicon = get_field('favicon', 'option');
  if (!empty($favicon)) { ?>
    <link rel="icon" type="image/png" href="<?php echo $favicon; ?>" sizes="32x32" />

  <?php } else { ?>
    <link rel="icon" type="image/png" href="<?php echo get_template_directory_uri(); ?>/assets/images/favicon.png" sizes="32x32" />
  <?php } ?>